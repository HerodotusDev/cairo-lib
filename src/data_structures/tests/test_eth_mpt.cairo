#[test]
#[available_gas(9999999999)]
fn test_hash_rlp_node() {
    let mut rlp_node = array![
        0x7a99fc8c339d66f8,
        0x65a125ac67212582,
        0x9f1beb530373d980,
        0xd0c4824c0ebf6b0c,
        0x96a0800144f846b8,
        0x2089d02a8ffbbdc4,
        0x66e96f21f693ad0b,
        0xd8bf551b76e2f952,
        0xa0f6ca6e3dad15a7,
        0x667a66e16ef9364e,
        0xa085d1c457acfa3d,
        0x9d07e017a2a369e3,
        0xc71a5df8340f6249
    ];

    let hash = MPTTrait::hash_rlp_node(rlp_node.span(), 8);
    assert(
        hash == 0x035F9A54E8BEE015293EB9791C7FEC6A4A111DB8B32464597B6F8E63B1167FA1,
        'Wrong node rlp hash'
    );
}

