#[test]
#[available_gas(9999999999)]
fn test_decode_rlp_node_leaf_odd() {
    let rlp_node = array![
        0x7a99fc8c339d66f8,
        0x65a125ac67212582,
        0x9f1beb530373d980,
        0xd0c4824c0ebf6b0c,
        0x96a0800144f846b8,
        0x2089d02a8ffbbdc4,
        0x66e96f21f693ad0b,
        0xd8bf551b76e2f952,
        0xa0f6ca6e3dad15a7,
        0x667a66e16ef9364e,
        0xa085d1c457acfa3d,
        0x9d07e017a2a369e3,
        0xc71a5df8340f6249
    ];

    let expected_key_end = array![
        0x2125827a99fc8c33, 0x73d98065a125ac67, 0xbf6b0c9f1beb5303, 0xd0c4824c0e
    ];
    let expected_value = array![
        0xbdc496a0800144f8,
        0xad0b2089d02a8ffb,
        0xf95266e96f21f693,
        0x15a7d8bf551b76e2,
        0x364ea0f6ca6e3dad,
        0xfa3d667a66e16ef9,
        0x69e3a085d1c457ac,
        0x62499d07e017a2a3,
        0xc71a5df8340f
    ];

    let (decoded, rlp_byte_len) = MPTTrait::decode_rlp_node(rlp_node.span()).unwrap();
    assert(rlp_byte_len == 13 * 8, 'Wrong RLP len');

    let expected_node = MPTNode::Leaf((expected_key_end.span(), expected_value.span(), 1, 57));
    assert(decoded == expected_node, 'Even leaf node differs');
}

